{
  "name": "agawamjavastop",
  "version": "2.0.2",
  "private": true,
  "author": "Jude Giordano",
  "homepage": "https://www.agawamjavastop.com",
  "description": "react application for local coffee chop",
  "keywords": [
    "next.js",
    "typescript",
    "node",
    "react",
    "mongodb"
  ],
  "license": "MIT",
  "scripts": {
    "docker:build": "docker build -t agawam-java-stop-dev .",
    "docker:run": "docker run -d --env-file .env --rm -p 3000:3000 agawam-java-stop-dev",
    "docker:stop": "docker stop ",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "postinstall": "npm run build",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p $PORT"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "axios": "^0.21.1",
    "cookie": "^0.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.0",
    "jsonwebtoken": "^8.5.1",
    "logrocket": "^1.2.0",
    "mongodb": "^3.6.6",
    "mongoose": "^5.12.7",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@types/cookie": "^0.4.0",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/react": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "eslint-plugin-react": "^7.23.2",
    "typescript": "^4.2.4"
  },
  "cacheDirectories": [
    ".next/cache"
  ]
}
